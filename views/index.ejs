<h2>Here's how you set up and push to Heroku.</h2>

<ul>
<li>git init</li>
<li>heroku login</li>
<li>heroku create</li>
<li>echo "web: node server.js" > Procfile</li>
<li>git add . </li>
<li>git commit -m "changes"</li>
<li>git push heroku main</li>
</ul>

<h1>AOE4 Units</h1>
<label>Choose two units to compare</label>
    <!-- <ul class="rappers"> -->
    <form action="/getSelectedUnitObject" method="GET">
        <select id="selectNumberOne" name="selectNumberOne">
            <option selected>Choose a unit</option>
            <% for(let i=0; i < info.length; i++) {%>
                
                
                <option id="selectedUnit" value="<%= JSON.stringify(info[i].data.id)  %>"> <%= JSON.stringify(info[i].data.id)  %> </option>
            
            
            <% } %>    
        <!-- </li> -->
        </select>
        <select id="selectNumberTwo" name="selectNumberTwo">
            <option selected>Choose a second unit</option>
            <% for(let i=0; i < info.length; i++) {%>
                
                
                <option id="selectedUnit2" value="<%= JSON.stringify(info[i].data.id)  %>"> <%= JSON.stringify(info[i].data.id)  %> </option>
            
            
            <% } %>    
        <!-- </li> -->
        </select>
        <button type="submit">Submit</button>
    </form>
    
    
    
    
    

<script src="Full-stack-template/public/main.js"></script>
